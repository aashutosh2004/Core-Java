import java.util.LinkedHashMap;
import java.util.Map;

public class fruitPrice_Map {
    public static void main(String[] args) {
        LinkedHashMap<String, Double> fruitPriceMap = new LinkedHashMap<>();

        fruitPriceMap.put("Apple", 120.50);
        fruitPriceMap.put("Banana", 40.00);
        fruitPriceMap.put("Cherry", 300.00);
        
        System.out.println("Fruit Price Map (Insertion Order):");
        for (Map.Entry<String, Double> entry : fruitPriceMap.entrySet()) {
            System.out.println(entry.getKey() + ": â‚¹" + entry.getValue());
        }
    }
}
